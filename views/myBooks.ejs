<div class="container">
    <table class="table table-dark">
        <thead>
          <tr>
            <th >Image</th>
            <th scope="col">Title</th>
            <th scope="col">Author</th>
            <th scope="col">Price</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
         
          <% for (let book of books) { %>
            <tr>
                <th scope="row"> <img class="img-fluid hover-zoom" width="50px" height="50px" src="upload/<%=book.img%>" alt=""></th>
                <td><%=book.title%></td>
                <td><%=book.author%></td>
                <td><%=book.price%> $</td>
                <td>
                    <a href="/myBooks/update/<%=book._id%> " class="btn btn-primary">update</a>
                    <a href="/myBooks/delete/<%=book._id%>" class="btn btn-danger">delete</a>
                </td>
               
              </tr>
              <%}%>
        </tbody>
      </table>
</div>
